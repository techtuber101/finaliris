# Production Environment Configuration
# Copy this file to .env.local and adjust values as needed

# Environment Mode - Set to 'production' for production deployment
NEXT_PUBLIC_ENV_MODE=production

# Frontend URL (production domain)
NEXT_PUBLIC_URL=https://irisvision.ai

# Backend API URL (production domain)
NEXT_PUBLIC_BACKEND_URL=https://irisvision.ai/api

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Optional: Google OAuth
NEXT_PUBLIC_GOOGLE_CLIENT_ID=

# Optional: PostHog Analytics
NEXT_PUBLIC_POSTHOG_KEY=

# Admin API Key
KORTIX_ADMIN_API_KEY=

# Optional: Edge Config
EDGE_CONFIG=
